{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "ELEVENLABS_API_KEY": "${ELEVENLABS_API_KEY}",
    "CALCOM_API_KEY": "${CALCOM_API_KEY}",
    "CALCOM_DOMAIN": "${CALCOM_DOMAIN}"
  }
}